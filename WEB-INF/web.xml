<web-app>

    <servlet>
        <servlet-name>ServletDBConnect</servlet-name>
        <servlet-class>Signup</servlet-class>  
    </servlet>

    <servlet>
        <servlet-name>ServletDB</servlet-name>
        <servlet-class>Main</servlet-class>  
    </servlet>
    <servlet>
        <servlet-name>Servlet</servlet-name>
        <servlet-class>Sign</servlet-class>  
    </servlet>
    <servlet>
        <servlet-name>Serv</servlet-name>
        <servlet-class>resume</servlet-class>  
    </servlet>
    <servlet>
        <servlet-name>Ser</servlet-name>
        <servlet-class>Final</servlet-class>  
    </servlet>


    <servlet-mapping>
            <servlet-name>ServletDBConnect</servlet-name>
            <url-pattern>/signin</url-pattern>
    </servlet-mapping>
    
   
    <servlet-mapping>
            <servlet-name>ServletDB</servlet-name>
            <url-pattern>/main</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
            <servlet-name>Servlet</servlet-name>
            <url-pattern>/login</url-pattern>
    </servlet-mapping>
     <servlet-mapping>
            <servlet-name>Serv</servlet-name>
            <url-pattern>/resume</url-pattern>
    </servlet-mapping>
     <servlet-mapping>
            <servlet-name>Ser</servlet-name>
            <url-pattern>/final</url-pattern>
    </servlet-mapping>


</web-app>